@model EntityLayer.Concrete.Admin
@{
    ViewBag.Title = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Admin Ekleme Sayfası</h1>
<script src=" https://unpkg.com/sweetalert/dist/sweetalert.min.js "></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
@using (Html.BeginForm("AddAdmin", "Authorization", FormMethod.Post))
{
<div>
    @Html.Label("Admin İsim-Soyisim")
    @Html.TextBoxFor(x => x.AdminUserName, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.AdminUserName)
    <br />
    @Html.Label("Şifre Yazınız")
    @Html.TextAreaFor(x => x.AdminPassword, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.AdminPassword)
    <br />
    @Html.Label("Rol Yazınız")
    @Html.TextAreaFor(x => x.AdminRole, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.AdminRole)
    <br />
    <button class="btn btn-primary" id="btn1">Kaydet</button>
</div>


}
<script>
    $('#btn1').click(function () {
        swal("Veri Kayıt Edilmiştir.", "", "success")
    })
</script>